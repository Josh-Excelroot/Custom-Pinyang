<odoo>
        <record id="einvoice_uom_form_view" model="ir.ui.view">
        <field name="name">einvoice.uom</field>
        <field name="model">einvoice.uom</field>
        <field name="arch" type="xml">
            <form string="E-Invoice Code">
                <group>
                    <field name="uom_code"/>
                    <field name="uom_name"/>
                </group>
            </form>
        </field>
    </record>

    <record id="einvoice_uom_tree_view" model="ir.ui.view">
        <field name="name">einvoice.uom</field>
        <field name="model">einvoice.uom</field>
        <field name="arch" type="xml">
            <tree string="E-Invoice Code">

                <field name="uom_code"/>
                <field name="uom_name"/>

            </tree>
        </field>
    </record>

    <record id="action_einvoice_uom" model="ir.actions.act_window">
        <field name="name">E Invoice UOM</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">einvoice.uom</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Create a new UOM code
            </p>
            <p>
                Create and manage the New Einvoice Uom .
            </p>
        </field>
    </record>

    <menuitem action="action_einvoice_uom" id="menu_einvoice_uom" parent="base.menu_users"/>

