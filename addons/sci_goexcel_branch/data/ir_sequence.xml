<odoo>
    <data noupdate="1">

        <record id="seq_vendor_deb_seq" model="ir.sequence">
            <field name="name">Vendor debit notes sequence</field>
            <field name="code">account.payment.customer.refund</field>
            <field name="prefix">APDN%(y)s%(month)s</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_cust_deb_seq" model="ir.sequence">
            <field name="name">Debit Note Sequence</field>
            <field name="code"></field>
            <field name="prefix">ARDN%(y)s%(month)s</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

