<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
        <!-- Sequence for Bank Deposits -->
        <record id="seq_bank_deposit" model="ir.sequence">
            <field name="name">Sequence for Bank Deposits</field>
            <field name="code">bank.deposit.sequence</field>
            <field name="prefix">BD-%(y)s%(month)s-</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence for Bank Withdrawals -->
        <record id="seq_bank_withdrawal" model="ir.sequence">
            <field name="name">Sequence for Bank Withdrawals</field>
            <field name="code">bank.withdrawal.sequence</field>
            <field name="prefix">BW-%(y)s%(month)s-</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
	</data>
</odoo>
