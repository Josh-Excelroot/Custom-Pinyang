<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="cust_plan_cust_seq" model="ir.sequence">
            <field name="name">customer technical plan</field>
            <field name="code">technical.customer.plan</field>
            <field name="prefix"></field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="gift_celibration_data_seq" model="ir.sequence">
            <field name="name">Gift Celibration</field>
            <field name="code">gift.celebration</field>
            <field name="prefix">GC</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record forcecreate="True" id="decimal_bps" model="decimal.precision">
            <field name="name">bps percentage</field>
            <field name="digits">1</field>
        </record>

        <!-- Gift Delivery Order -->
        <record id="gift_delivery_order_seq" model="ir.sequence">
            <field name="name">Gift Delivery Order</field>
            <field name="code">gift.delivery.order</field>
            <field name="prefix">%(y)s-</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>