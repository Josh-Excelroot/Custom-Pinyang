	<record id="buying_journary_form_view" model="ir.ui.view">
		<field name="name">buying.journary.form</field>
		<field name="model">buying.journary</field>
		<field name="arch" type="xml">
			<form string="Buyer journey">
				<sheet>
					<group>
						<group>
							<field name="name" required="1"/>
							<field name="stage_id" options="{'no_create': True, 'no_edit': True, 'no_create_edit': True}"/>
							<field name="active"/>
						</group>
						<group>
							<field name="description"/>
							<field name="sale_journay_id" options="{'no_create': True, 'no_edit': True, 'no_create_edit': True}"/>
							<field groups="base.group_multi_company" invisible="1" name="company_id" options="{'no_create': True, 'no_edit': True, 'no_create_edit': True}"/>
						</group>
					</group>
				</sheet>
			</form>
		</field>
	</record>
