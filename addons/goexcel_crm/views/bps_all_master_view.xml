	<record id="discovery_sheet_form_view" model="ir.ui.view">
		<field name="name">discovery.sheet.form</field>
		<field name="model">discovery.sheet</field>
		<field name="arch" type="xml">
			<form string="Discovery Sheet">
				<sheet>
					<group>
						<field name="name"/>
						<group>
							<field name="sector_id"/>
						</group>
						<group>
							<field name="industry_id"/>
							<field groups="base.group_multi_company" invisible="1" name="company_id" options="{'no_create': True, 'no_edit': True, 'no_create_edit': True}"/>
						</group>
					</group>
					<field mode="tree, kanban, form" name="product_line_ids">
						<tree editable="bottom">
							<field name="machine_id"/>
							<field name="product_group_id" options="{'no_create': True, 'no_edit': True, 'no_create_edit': True}"/>
							<field name="product_category_id"  options="{'no_create': True, 'no_edit': True, 'no_create_edit': True}"/>
							<field name="product_ids" options="{'no_create': True, 'no_edit': True, 'no_create_edit': True}" domain="[('categ_id', '=', product_category_id)]" widget="many2many_tags"/>
						</tree>
						<form>
							<field name="machine_id"/>
							<field name="product_group_id" options="{'no_create': True, 'no_edit': True, 'no_create_edit': True}"/>
							<field name="product_category_id"  options="{'no_create': True, 'no_edit': True, 'no_create_edit': True}"/>	
							<field name="product_ids" domain="[('categ_id', '=', product_category_id)]" widget="many2many_tags"/>
						</form>
						<kanban class="o_kanban_mobile">
							<field name="machine_id"/>
							<field name="product_group_id"/>
							<field name="product_category_id"/>
							<field name="product_ids"/>
							<templates>
								<t t-name="kanban-box">
									<div t-attf-class="oe_kanban_global_click">
										<div>
											<strong class="o_kanban_record_title">SKU:
												<t t-esc="record.product_id.value"/></strong>
										</div>
										<div t-if="record.machine_id.value">Machine:
											<t t-esc="record.machine_id.value"/></div>
										<div t-if="record.product_group_id.value">Group:
											<t t-esc="record.product_group_id.value"/></div>
										<div t-if="record.product_category_id.value">Category:
											<t t-esc="record.product_category_id.value"/></div>
									</div>
								</t>
							</templates>
						</kanban>
					</field>
				</sheet>
			</form>
		</field>
	</record>
