	<!-- Customer Technical Plan -->
	<record id="technical_customer_plan_form_view" model="ir.ui.view">
		<field name="name">technical.customer.plan.form</field>
		<field name="model">technical.customer.plan</field>
		<field name="arch" type="xml">
			<form string="Customer Technical Plan">
				<header>
					<button class="btn-primary" name="action_to_approve" string="To Approve" type="object" attrs="{'invisible': [('state', '!=', 'draft')]}"/>
					<button class="btn-primary" name="action_to_approved" string="Approve" type="object" attrs="{'invisible': ['|', ('state', '!=', 'to_approve'), ('is_approver', '=', False)]}"/>
					<button class="btn-primary" name="action_done" string="Done" type="object" attrs="{'invisible': [('state', '!=', 'approved')]}"/>
					<button class="btn-primary" name="action_reject" string="Reject" type="object" attrs="{'invisible': [('state', 'not in', ['to_approve'])]}"/>
					<button class="btn-primary" name="action_to_draft" string="Reset To Draft" type="object" attrs="{'invisible': [('state', 'in', ['approved', 'to_approve', 'draft'])]}"/>
					<button class="btn-primary" name="action_cancel" string="Cancel" type="object" attrs="{'invisible': [('state', 'not in', ['draft', 'cancel'])]}"/>
					<button class="btn-primary" name="send_reminder" string="Send Mail" type="object" attrs="{'invisible': [('state', '=', 'draft')]}"/>
					<field name="state" widget="statusbar"/>
				</header>
				<sheet>
					<div class="oe_title">
						<lable class="oe_edit_only" for="name"/>
						<h1>
							<field default_focus="1" name="name" placeholder="Technical Plan No"/>
						</h1>
					</div>
					<group>
						<group>
							<field name="technical_plan" required="1"/>
							<field name="date" required="1"/>
							<field name="volume_per_month"/>
							<field name="remark"/>
							<field name="involved_parties_ids" options="{'no_create': True, 'no_edit': True, 'no_create_edit': True}" widget="many2many_tags"/>
							<field attrs="{'invisible': [('approved_by', '=', False)]}" name="approved_by" options="{'no_create': True, 'no_edit': True, 'no_create_edit': True}"/>
							<field name="is_approver" invisible="1"/>
						</group>
						<group>
							<field domain="[('customer', '=', True), ('type', '!=', 'related_company')]" name="partner_id" required="1"/>
							<field domain="[('parent_id', '=', partner_id), ('type', '!=', 'related_company')]" name="contact_person_ids" options="{'no_create': True, 'no_edit': True, 'no_create_edit': True}" widget="many2many_tags"/>
							<field name="owner_id"/>
							<field name="schedule_reminder" widget="date"/>
							<field groups="base.group_multi_company" invisible="1" name="company_id" options="{'no_create': True, 'no_edit': True, 'no_create_edit': True}"/>
							<field attrs="{'invisible': [('rejected_by', '=', False)]}" name="rejected_by" options="{'no_create': True, 'no_edit': True, 'no_create_edit': True}"/>
						</group>
					</group>
					<group>
						<field name="purpose" placeholder="Why are you making this visit ?"/>
						<field name="objective" placeholder="What result do you expect from this visit?"/>
						<field name="premise" placeholder="What do you know about the customer?"/>
						<field name="strategy" placeholder="How you will achive this ?"/>
						<field name="anticipate" placeholder="What if/else"/>
					</group>
				</sheet>
			</form>
		</field>
	</record>
