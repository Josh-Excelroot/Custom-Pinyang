<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="credit_limit_app_seq" model="ir.sequence">
            <field name="name">Credit Limit Request</field>
            <field name="code">partner.credit.approval</field>
            <field name="prefix">CLR</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="cash_sale_delivery_seq" model="ir.sequence">
            <field name="name">Cash Sale Delivery</field>
            <field name="code">cash.sale.delivery</field>
            <field name="prefix">WH/CASH/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="cash_sale_seq" model="ir.sequence">
            <field name="name">Cash Sale</field>
            <field name="code">cash.sale</field>
            <field name="prefix">CSH-%(y)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>